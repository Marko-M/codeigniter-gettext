{
    "name": "joel-depiltech/codeigniter-gettext",
    "description": "A Codeigniter PHP framework package for dealing with gettext as a Composer requirement.",
    "time": "2016-12-23",
    "type": "library",
    "license": "GPL-2.0",
    "keywords": ["codeigniter", "gettext", "i18n", "translation", "language"],
    "authors": [
        {
            "name": "Joël Gaujard",
            "email": "joel@depiltech.com"
        },
        {
            "name": "Joël Gaujard",
            "email": "j.gaujard@gmail.com"
        }
    ],
    "homepage": "https://github.com/joel-depiltech/codeigniter-gettext",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "classmap": ["src/"],
        "psr-4": {
            "CodeIgniterGetText\\Tests\\": "tests/"
        },
        "files": [
            "tests/include/gettext_test_functions.php",
            "tests/include/gettext_test_constants.php"
        ]
    }
